{
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node",
    "transpileOnly": true,
    "files": true
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "src",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "module": "ESNext"
  },
  "include": ["./src/"],
  "exclude": ["node_modules"],
  "module": "ES2020"
}
